 <script>
 document.getElementById("but1").addEventListener("click", function () {
 event.preventDefault()
 var name1 = document.getElementById("1").value;
 var Age1 = document.getElementById("2").value;
 var Weight1 = document.getElementById("3").value;
 var Type1 = document.getElementById("4").value;
 var what1 = document.getElementById("5").value;
 var pet = {
 name: name1,
 age: Age1,
 weight: Weight1,
 type: Type1,
 what: what1
 }
